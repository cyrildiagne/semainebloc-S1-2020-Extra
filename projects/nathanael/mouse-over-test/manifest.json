{
    "manifest_version": 2,
    "name": "test",
    "version": "0.1",
    "content_scripts": [{
        "matches": [
            "<all_urls>"
        ],
        "js": ["content.js"]
    }],

    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "ico.png",
        "default_popup": "popup.html",
        "default_title": "A popup will end all things."
    }
}