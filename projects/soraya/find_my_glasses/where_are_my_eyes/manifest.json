{
  "name": "Find my glasses",
  "version": "0.2",
  "description": "Unblurr my vision",
  "background": {
    "persistent": false,
    "scripts": ["js/background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "all_frames": true,
      "js": ["js/content.js","js/jquery-3.5.1.min.js"],
      "css": ["css/content.css"],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_icon": "icon_eye.png"
  },
  "manifest_version": 2
}
